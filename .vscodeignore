# ignore editor and Git metadata
.vscode/**
.git/**

# ignore IDE tool configs
.eslintrc.js
eslint.config.js
.eslintignore
.prettierrc*
tsconfig.json
tsconfig.*.json
vitest.config.ts
esbuild.mjs

# ignore source code and tests
src/**
test/**
